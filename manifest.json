{
    "name": "Speed it up",
    "version": "0.0.1",
    "description": "Adds a browser action that provides a way to adjust playback rate on any website",
    "icons": {
        "16" : "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "browser_action": {
        "default_title": "Speed it up",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}

